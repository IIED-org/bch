{#
/**
 * @file
 * Default theme implementation for horizontal tabs.
 *
 * Available variables
 * - attributes: A list of HTML attributes for the wrapper element.
 * - children: The rendered children.
 *
 * @see template_preprocess_horizontal_tabs()
 *
 * @ingroup themeable
 */
#}
<div data-horizontal-tabs class="container clearfix px-4 mx-auto sm:px-0">
  <ul data-horizontal-tabs-list class="flex flex-col mb-1 space-y-4 lg:space-y-0 lg:space-x-4 lg:flex-row visually-hidden"></ul>
  <div data-horizontal-tabs-panes{{ attributes }} class="p-4 prose bg-white rounded-3xl md:p-6 ">{{ children }}</div>
</div>
